{
  "name": "geospatial-platform-client",
  "version": "1.0.0",
  "description": "Frontend application for Geospatial Visualization Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "electron:start": "electron .",
    "electron:build": "electron-builder",
    "cap:build": "npm run build && cap sync",
    "cap:ios": "npm run cap:build && cap open ios",
    "cap:android": "npm run cap:build && cap open android"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "axios": "^1.6.2",
    "mapbox-gl": "^3.0.1",
    "react-map-gl": "^7.1.7",
    "deck.gl": "^8.9.35",
    "@deck.gl/react": "^8.9.35",
    "@deck.gl/layers": "^8.9.35",
    "@deck.gl/geo-layers": "^8.9.35",
    "@deck.gl/aggregation-layers": "^8.9.35",
    "cesium": "^1.112.0",
    "resium": "^1.17.0",
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "socket.io-client": "^4.6.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/android": "^5.5.1",
    "electron": "^28.0.0",
    "chart.js": "^4.4.1",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "@capacitor/cli": "^5.5.1",
    "electron-builder": "^24.9.1"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
